    "LICENSE",
    "package.json"
  ],
  "types": "types/index.d.ts",
  "dependencies": {
    "@func-js/utils": "^0.0.5"
  },
  "devDependencies": {
    "webpack": "^5.7.0",
    "webpack-cli": "^4.2.0",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "babel-loader": "^8.2.2"
  }
}
