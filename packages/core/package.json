{
  "name": "@func-js/core",
  "version": "0.0.5",
  "description": "the core package of func-js",
  "main": "dist/func.core.js",
  "_sourceName": "func.core",
  "author": "Cangshi",
  "license": "MIT",
  "scripts": {
    "build-source": "webpack --progress --env production=false ",
